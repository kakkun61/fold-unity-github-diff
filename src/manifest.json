{
    "manifest_version": 2,
    "name": "chrome extension to fold diff of Unity specified files on GitHub",
    "version": "1",
    "author": "Kazuki Okamoto",
    "content_scripts": [
        {
            "matches": [
                "https://*/*"
            ],
            "js": [
                "contentscript.js"
            ]
        }
    ],
    "web_accessible_resources": [
        "script.js"
    ]
}
